<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="MonthViewController">
	<children>
		<VBox alignment="CENTER" prefHeight="80" prefWidth="890">
			<Label fx:id="monthName" text="Testmonth" alignment="BOTTOM_CENTER"/>
		</VBox>
		<GridPane layoutY="80" alignment="BOTTOM_CENTER">
			<columnConstraints>
				<ColumnConstraints prefWidth="50" />
				<ColumnConstraints prefWidth="120" />
				<ColumnConstraints prefWidth="120" />
				<ColumnConstraints prefWidth="120" />
				<ColumnConstraints prefWidth="120" />
				<ColumnConstraints prefWidth="120" />
				<ColumnConstraints prefWidth="120" />
				<ColumnConstraints prefWidth="120" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints prefHeight="50" />
				<RowConstraints prefHeight="110" />
				<RowConstraints prefHeight="110" />
				<RowConstraints prefHeight="110" />
				<RowConstraints prefHeight="110" />
				<RowConstraints prefHeight="110" />
				<RowConstraints prefHeight="110" />
			</rowConstraints>
			<children>
				<Label text="Week" GridPane.columnIndex="0" GridPane.rowIndex="0" />
				<Label text="Monday" GridPane.columnIndex="1"
					GridPane.rowIndex="0" />
				<Label text="Tuesday" GridPane.columnIndex="2"
					GridPane.rowIndex="0" />
				<Label text="Wednesday" GridPane.columnIndex="3"
					GridPane.rowIndex="0" />
				<Label text="Thursday" GridPane.columnIndex="4"
					GridPane.rowIndex="0" />
				<Label text="Friday" GridPane.columnIndex="5"
					GridPane.rowIndex="0" />
				<Label text="Saturday" GridPane.columnIndex="6"
					GridPane.rowIndex="0" />
				<Label text="Sunday" GridPane.columnIndex="7"
					GridPane.rowIndex="0" />
			</children>
		</GridPane>
	</children>
</AnchorPane>